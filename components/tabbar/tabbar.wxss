.comp-tabbar{
  position:fixed;bottom:0;
  display:flex;
  align-items:center;
  width:100%;
  height:94rpx;
  box-sizing:border-box;
  border-top:1rpx solid;
  text-align:center;
  font-size:22rpx;
}
.comp-tabbar .tabbarItem{
  flex:1;height:inherit;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:center;
}
.comp-tabbar .tabbarItem .icon{
  width:50rpx;
  height:50rpx;
}
